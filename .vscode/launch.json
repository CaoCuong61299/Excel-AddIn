	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch server (development)",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/server/main.js",
			"cwd": "${workspaceFolder}/server"
		},

		{
			"name": "Build server distribution",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/server/build-project.js",
			"cwd": "${workspaceFolder}/server"
		}
	]
}